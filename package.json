{
  "name": "vision-camera-facedetector",
  "version": "1.3.3",
  "description": "VisionCamera Frame Processor Plugin that uses MLKit Face Recognition API to recognize faces",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "VisionCameraFaceDetector.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "*.podspec",
    "app.plugin.js",
    "!**/__tests__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "prepare": "bob build",
    "release": "release-it",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn && yarn example && yarn example pods && yarn configure",
    "start": "echo \"This is a RN module. To run the example, use 'yarn example start'\"",
    "example": "yarn workspace TfliteExample"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/UN-ICC/vision-camera-facedetector",
  "author": "United Nations International Computing Centre (https://github.com/UN-ICC)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UN-ICC/vision-camera-facedetector/issues"
  },
  "homepage": "https://github.com/UN-ICC/vision-camera-facedetector#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.2",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/jest": "^26.0.0",
    "@types/react": "~29.5.11",
    "@types/react-native": "0.73.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "pod-install": "^0.1.26",
    "prettier": "^3.2.4",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-builder-bob": "^0.23.2",
    "release-it": "^17.0.3",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example"
  ],
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "prettier": {
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "@react-native/babel-preset": "^0.73.20"
  }
}
